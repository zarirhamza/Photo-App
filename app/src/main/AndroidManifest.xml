<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidapp31">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Photo Android App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".displayPhoto"
            android:label="@string/title_activity_display_photo"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".photomain"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PhotoListView"
            android:label="@string/title_activity_photo_list_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Listview"
            android:label="@string/title_activity_list_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>